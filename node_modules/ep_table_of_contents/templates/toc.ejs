<div id ="CharacterSheetPlugin">  


<div id="myform">
<h2><b>Character Creation</b></h2>
<table>
    <tr>
        <td>Name:</td>
		<td><input type="text" id="name"></td>
    </tr>
	
	<tr>
        <td>Gender:</td>
		<td><select id="sex">  
			<option selected="" value="Default">(Please select a gender)</option>  
			<option value="Male">Male</option>  
			<option value="Female">Female</option>  
			<option value="Others">Others</option>  
			</select>
		</td>
    </tr>
    
	<tr>
        <td>Appearance:</td>
        <td>
			<TEXTAREA id="appearance" 
			   ROWS="3" COLS="25">

			</TEXTAREA>
		</td>
    </tr>
	
	<tr>
        <td>Personality/Backstory:</td>
        <td><TEXTAREA id="personalityBackstory" 
			   ROWS="3" COLS="25">

			</TEXTAREA></td>
    </tr>
	
	<tr>
		<td>
		<input type="button" id="create" value="Click here" onclick="Javascript:addTable()">
	</tr>
    
	<tr>
        <td>&nbsp;</td>
        <td>&nbsp;</td>
    </tr>
</table>

</div>

